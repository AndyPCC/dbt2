# Makefile.in
# 31 October 2002

include ../make.common

OBJS =	common.so transaction_data.so _socket.so client_interface.so logging.so

all: $(OBJS)

_socket.so: $(INCLUDEDIR)/_socket.h

common.so: $(INCLUDEDIR)/common.h

logging.so: $(INCLUDEDIR)/logging.h $(INCLUDEDIR)/common.h

transaction_data.so: $(INCLUDEDIR)/common.h $(INCLUDEDIR)/transaction_data.h

client_interface.so: $(INCLUDEDIR)/client_interface.h

clean: 
	rm -f $(OBJS)

plot "vmstat.out.all.data" title "total" with lines, "vmstat.out.us.data" title "user" with lines, "vmstat.out.sy.data" title "system" with lines, "vmstat.out.id.data" title "idle" with lines, "vmstat.out.wa.data" title "wait" with lines
set xlabel "Elapsed Time (s)"
set ylabel "% Utilized"
set term png small color
set output "cpu.png"
set yrange [0:100]
replot
